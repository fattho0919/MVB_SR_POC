{
  "model": {
    "default_model_path": "models/DSCF_720p_x2_int8.tflite",
    "alternative_models": [
      "models/DSCF_float32.tflite",
      "models/DSCF_float16.tflite",
      "models/DSCF_int8.tflite",
      "models/DSCF_720p_x2_int8.tflite"
    ],
    "expected_scale_factor": 2,
    "channels": 3
  },
  "processing": {
    "default_num_threads": 4,
    "use_xnnpack": true,
    "allow_fp16_precision": true,
    "use_nnapi": true,
    "enable_pure_cpu_mode": true,
    "cpu_thread_affinity": true,
    "cpu_optimization_level": "HIGH",
    "gpu_inference_preference": "FAST_SINGLE_ANSWER",
    "gpu_precision_loss_allowed": true,
    "gpu_wait_type": "PASSIVE",
    "gpu_backend": "OPENCL",
    "enable_quantized_inference": true,
    "experimental_gpu_optimizations": true
  },
  "npu": {
    "enable_npu": true,
    "allow_fp16_on_npu": true,
    "npu_accelerator_name": "",
    "use_npu_for_quantized": true,
    "execution_preference": "SUSTAINED_SPEED",
    "enable_npu_prewarming": true,
    "npu_warmup_timeout_ms": 3000,
    "cache_npu_compilation": true,
    "npu_memory_alignment_bytes": 64,
    "enable_npu_operation_partitioning": true
  },
  "tiling": {
    "overlap_pixels": 32,
    "memory_threshold_percentage": 0.6,
    "max_input_size_without_tiling": 2048,
    "force_tiling_above_mb": 500
  },
  "memory": {
    "low_memory_warning_mb": 100,
    "gc_after_inference": false,
    "enable_memory_logging": true
  },
  "ui": {
    "default_tiling_enabled": false,
    "show_detailed_timing": true,
    "show_memory_stats": true
  },
  "debugging": {
    "enable_verbose_logging": false,
    "log_model_shapes": true,
    "log_performance_stats": true,
    "enable_buffer_validation": false
  }
}